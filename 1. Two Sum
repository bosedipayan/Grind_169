class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> ans;
        int n=nums.size();
        
        unordered_map<int,int> mp;
        
        for(int i=0;i<n;i++)
        {
            int res=target-nums[i];
            
            if(mp.find(res)!=mp.end())
            {
                ans.push_back(mp[res]);
                ans.push_back(i);
            }
            mp.insert({nums[i],i});
        }
        return ans;
    }
};
