class Solution {
public:
    int missingNumber(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        
        int n=nums.size(),i=0;
        for(i=0;i<n;i++)
        {
            if(nums[i] == i) continue;
            else{
                break;
            }
        }
        return i;
    }
};
